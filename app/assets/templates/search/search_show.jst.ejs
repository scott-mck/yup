<div class="filters">
  <p>Filters:</p>

  <form>
    <div class="filter">Alphabetically <input class="name" type="checkbox"
      <% if (order == "name") { %>
        checked
      <% } %>>
    </div>

    <div class="filter">Most reviewed <input class="review_count" type="checkbox"
      <% if (order == "review_count") { %>
        checked
      <% } %>>
    </div>

    <div class="filter">Highest rated <input class="rating" type="checkbox"
      <% if (order == "rating") { %>
        checked
      <% } %>>
    </div>
  </form>
</div>
<div class="categories"></div>
<h1 class="query">Searched for "<%= query %>"</h1>
<div class="businesses">
  <% if (businesses && businesses.length == 0) { %>
    <h1>No results</h1>
  <% } %>
</div>
