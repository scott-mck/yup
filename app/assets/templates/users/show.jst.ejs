<div class="title">
  <%= user.escape('first_name') + " " %>
  <%= user.escape('last_name') + "'s profile" %>
</div class="title">

<ul class="nav nav-tabs">
  <li class="reviews" role="presentation"><a href="#users/<%= user.get('id') %>">Reviews</a></li>
  <li class="followers" role="presentation"><a href="#users/<%= user.get('id') %>">Followed Users</a></li>
  <% if (user.get('id') == CURRENT_USER_ID) { %>
    <li class="edit" role="presentation">
      <a href="#users/<%= user.get('id') %>"=>Edit Profile</a>
    </li>
  <% } %>
</ul>

<div class="user-sidebar"></div>
<div class="user-main"></div>
